{
  "name": "BMR Calculator",
  "description": "A simple Energy Requirement calculator Google Chrome Extension.",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "windows"
  ],
  "background": {
    "service_worker": "src/background.js"
  },
  "action": {
    "default_popup": "index.html",
    "default_icon": "icon.png"
  }
}